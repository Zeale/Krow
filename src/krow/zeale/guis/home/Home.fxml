<BorderPane minHeight="600.0" minWidth="800.0" prefHeight="800.0"
	prefWidth="1200.0" styleClass="background-center-pane" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="krow.zeale.guis.home.HomeWindow">
	<top>
		<MenuBar fx:id="menuBar" focusTraversable="false"
			BorderPane.alignment="CENTER">
			<menus>
				<Menu mnemonicParsing="false" text="Kröw">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#onCloseRequested"
							text="Close" />
						<MenuItem mnemonicParsing="false" onAction="#onGoToPages"
							text="Pages" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="New">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#onGoToCreateConstructWindow"
							text="• Construct" />
						<MenuItem mnemonicParsing="false" text="• Program" />
						<MenuItem mnemonicParsing="false" onAction="#onGoToCreateSystemWindow"
							text="• System" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem mnemonicParsing="false" text="• Family" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem mnemonicParsing="false" text="• Declaration" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem mnemonicParsing="false" onAction="#onGoToCreateLawWindow"
							text="• Law" />
						<MenuItem mnemonicParsing="false" text="• Policy" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<Menu mnemonicParsing="false" text="• Module">
							<items>
								<MenuItem mnemonicParsing="false" text="Create..." />
								<MenuItem mnemonicParsing="false" text="• Patch" />
							</items>
						</Menu>
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem mnemonicParsing="false" text="• Area" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem mnemonicParsing="false" text="• Task" />
						<MenuItem mnemonicParsing="false" text="• Event" />
						<SeparatorMenuItem mnemonicParsing="false" />
						<Menu mnemonicParsing="false" text="• Ruleset">
							<items>
								<MenuItem mnemonicParsing="false" text="Blank Ruleset" />
							</items>
						</Menu>
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem mnemonicParsing="false" text="• Mindset" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="File">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#restore"
							text="Restore" />
						<MenuItem mnemonicParsing="false" onAction="#importFile"
							text="Import" />
						<MenuItem mnemonicParsing="false" onAction="#exportFile"
							text="Export" />
						<MenuItem mnemonicParsing="false" onAction="#backup"
							text="Backup" />
					</items>
				</Menu>
				<Menu mnemonicParsing="false" text="Tools">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#_event_openCalculator"
							text="Calculator" />
					</items>
				</Menu>
			</menus>
			<tooltip>
				<Tooltip text="This is the Home window of the application" />
			</tooltip>
		</MenuBar>
	</top>
	<center>
		<AnchorPane fx:id="centerPane" prefHeight="200.0"
			prefWidth="200.0" styleClass="center-pane" BorderPane.alignment="CENTER">
			<children>
				<Text fx:id="constructCount" layoutX="5.0" layoutY="134.0"
					strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="590.10205078125">
					<font>
						<Font name="System Bold" size="42.0" />
					</font>
				</Text>
				<Text fx:id="constructText" layoutX="252.0" layoutY="187.0"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Constructs">
					<font>
						<Font name="System Bold" size="21.0" />
					</font>
				</Text>
				<ImageView fx:id="krow" fitHeight="128.0" fitWidth="128.0"
					layoutX="240.0" layoutY="220.0" onMouseEntered="#onMouseEnteredKrowImage"
					onMouseExited="#onMouseExitedKrowImage" opacity="0.1" pickOnBounds="true"
					preserveRatio="true">
					<image>
						<Image url="@../../Kröw_hd.png" />
					</image>
					<effect>
						<DropShadow blurType="GAUSSIAN" height="70.76" radius="35.6575"
							spread="0.23" width="73.87" />
					</effect>
				</ImageView>
				<MenuButton layoutX="257.0" layoutY="421.0"
					mnemonicParsing="false" text="Manage">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#onGoToManageConstructWindow"
							text="Constructs" />
						<MenuItem mnemonicParsing="false" onAction="#onGoToManageLawWindow"
							text="Laws" />
					</items>
				</MenuButton>
			</children>
		</AnchorPane>
	</center>
	<bottom>
		<ScrollPane fx:id="bottomPane" focusTraversable="false"
			hbarPolicy="NEVER" pannable="true" prefHeight="200.0" prefWidth="1212.0"
			styleClass="bottom-pane" BorderPane.alignment="CENTER">
			<content>
				<VBox prefHeight="191.0" prefWidth="1181.0" />
			</content>
		</ScrollPane>
	</bottom>
	<left>
		<TableView fx:id="laws" focusTraversable="false" prefHeight="568.0"
			prefWidth="300.0" styleClass="side-tables" BorderPane.alignment="CENTER">
			<columns>
				<TableColumn fx:id="lawName" editable="false" maxWidth="150.0"
					minWidth="100.0" prefWidth="100.0" resizable="false" text="Law" />
				<TableColumn fx:id="lawDesc" editable="false" maxWidth="900.0"
					minWidth="150.0" prefWidth="190.0" resizable="false" text="Description" />
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
			<tooltip>
				<Tooltip text="This is a table of the Laws that you have" />
			</tooltip>
		</TableView>
	</left>
	<right>
		<TableView fx:id="constructs" focusTraversable="false"
			prefHeight="568.0" prefWidth="300.0" styleClass="side-tables"
			BorderPane.alignment="CENTER">
			<columns>
				<TableColumn fx:id="constName" editable="false"
					maxWidth="150.0" minWidth="100.0" prefWidth="100.0" resizable="false"
					text="Construct" />
				<TableColumn fx:id="constDesc" editable="false"
					maxWidth="900.0" minWidth="0.0" prefWidth="191.0" resizable="false"
					text="Description" />
			</columns>
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
			<tooltip>
				<Tooltip text="This is a List of the Constructs that you have" />
			</tooltip>
		</TableView>
	</right>
	<stylesheets>
		<URL value="@../_resources/DefaultTheme.css" />
		<URL value="@Home.css" />
	</stylesheets>
</BorderPane>
